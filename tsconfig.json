{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "ESNext",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "sourceMap": true,
      "target": "es5",
      "plugins": [
        {
          "name": "typescript-styled-plugin",
          "tags": [
            "styled",
            "css"
          ]
        }
      ]
  },
  "exclude": [
      "node_modules"
  ],
}